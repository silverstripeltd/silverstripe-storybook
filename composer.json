    "license": "BSD-3-Clause",
    "keywords": [
        "silverstripe",
        "storybook"
    ],
    "authors": [
        {
            "name": "Will Rossiter",
            "homepage": "https://www.fullscreen.io"
        }
    ],
    "require": {
        "silverstripe/framework": "^4.8",
